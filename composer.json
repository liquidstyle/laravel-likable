{
	"name": "liquidstyle/laravel-likeable",
	"description": "Traits for Laravel Eloquent models to allow easy implementation of a 'like' or 'favorite' and 'wishlist' feature.",
	"license": "MIT",
	"homepage": "https://github.com/liquidstyle",
	"keywords": ["trait", "laravel", "laravel5", "eloquent", "likeable", "likable", "like", "wishlist"],
	"authors": [
		{
			"name": "Jaye Miller",
			"email": "jaye.miller@liquidstyledigital.com"
		}
	],
	"require": {
		"php": ">=5.5.0",
		"illuminate/database": ">=5.0",
		"illuminate/support": ">=5.0"
	},
	"require-dev": {
		"orchestra/testbench": "~3.0",
		"phpunit/phpunit": "~4.0",
		"mockery/mockery": "~0.9"
	},
	"autoload": {
		"psr-4": {
			"Conner\\Likeable\\": "src/"
		}
	},
	"autoload-dev": {
		"classmap": [
			"tests/TestCase.php"
		]
	}
}
